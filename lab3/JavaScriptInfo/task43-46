let user = {};
user.name = "John";
user.surname: "Smith";
user.name = "Pete";
delete user.name;

2.
function isEmpty(obj) {
    for (let key in obj) {
        return false;
    }
    return true;
}

3.
let sum = 0;
for (let key in salaries) {
    sum += salaries[key];
}

4.
function multiplyNumeric(obj) {
    for (let key in obj) {
        if (typeof obj[key] == 'number') {
            obj[key] *= 2;
        }
    }
}